<Window x:Class="CircleAppV2.ArtistWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CircleAppV2"
        mc:Ignorable="d"
        Title="ArtistWindow" Height="450" Width="800">
    <DataGrid x:Name="workerDataGrid" AutoGenerateColumns="False" EnableRowVirtualization="True" ItemsSource="{Binding Workers}" Margin="10">
        <DataGrid.Columns>
            <DataGridTextColumn x:Name="IdColumn" Binding="{Binding ID}" Header="ID" Width="SizeToHeader"/>
            <DataGridTextColumn x:Name="Id2Column" Binding="{Binding ID_Perfomance}" Header="ID2" Width="SizeToHeader"/>
            <DataGridTextColumn x:Name="Id3Column" Binding="{Binding ID_Artist}" Header="Name" Width="SizeToHeader"/>
            <DataGridTextColumn x:Name="time" Binding="{Binding time}" Header="Time" Width="SizeToHeader"/>
        </DataGrid.Columns>
    </DataGrid>
</Window>
