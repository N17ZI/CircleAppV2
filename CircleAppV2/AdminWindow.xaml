<Window x:Class="CircleAppV2.AdminWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CircleAppV2"
        mc:Ignorable="d"
        Title="AdminWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto" MinHeight="58"/>
            <RowDefinition Height="Auto" MinHeight="83"/>
            <RowDefinition Height="Auto" MinHeight="83"/>
            <RowDefinition Height="Auto" MinHeight="76"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Label>Welcome admin</Label>
        <Button Margin="142,17,142,18" Grid.Row="1" Click="RegUser_Click" Content="New Employee"/>
        <Button Margin="142,17,142,18" Grid.Row="2" Click="NewAnimal_Click" Content="New animal"/>
        <Button Margin="142,14,142,14" Grid.Row="3" Click="Employes_Click" Content="Employees"/>
        <Button Margin="142,10,142,76" Grid.Row="4" Click="NewTask_Click" Content="New Task"/>
    </Grid>

</Window>
